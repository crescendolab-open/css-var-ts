{
  "allowCompoundWords": true,
  "enableFiletypes": ["*"],
  "enableGlobDot": true,
  "files": ["**/*"],
  "ignorePaths": [
    "**/*.local",
    "**/*.local.*",
    "**/*.svg",
    "**/.gitignore",
    "**/__snapshots__/**",
    "**/CHANGELOG.md",
    "**/LICENSE",
    "**/storybook-static",
    "/*.code-workspace",
    "/.changeset",
    "/.git",
    "/.prettierignore",
    "/.stylelintignore",
    "/pnpm-lock.yaml"
  ],
  "includeRegExpList": ["[ -~]"],
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "antfu",
    "commitizen",
    "esbuild",
    "gruvbox",
    "jiti",
    "jsdoc",
    "nums",
    "nvmrc",
    "pathe",
    "slugifies",
    "tsdoc",
    "tsdown",
    "unbundle",
    "unrs",
    "vite",
    "vitejs",
    "vitest"
  ]
}
